{
  "version": 2,
  "buildCommand": "cd apps/web-dashboard && npm install && npm run build",
  "outputDirectory": "apps/web-dashboard/.next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/web-dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://web-production-d9582.up.railway.app/api"
  },
  "github": {
    "enabled": true,
    "silent": false
  }
}
