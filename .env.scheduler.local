# Scheduler Local Environment Variables
# Импортируем основной .env файл

# Переопределяем переменные для локальной разработки
NODE_ENV=development
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/vendhub
REDIS_URL=redis://localhost:6379

# Специфичные переменные для Scheduler
SCHEDULER_QUEUE_NAME=tasks
SCHEDULER_KEY_PREFIX=scheduler
SCHEDULER_TIMEZONE=Asia/Tashkent
